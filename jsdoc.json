{
  "plugins": ["jsdoc-ts-utils", "plugins/markdown"],
  "source": {
    "include": ["./README.md", "./index.mjs"],
    "includePattern": ".+\\.((m|c)?js(doc|x)?|md)$"
  },
  "opts": {
    "mainpagetitle": "Homey Log",
    "destination": "./jsdoc"
  },
  "tsUtils": {
    "typeScriptUtilityTypes": false
  }
}
