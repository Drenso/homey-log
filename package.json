{
  "name": "@drenso/homey-log",
  "version": "10.39.0",
  "description": "Interface with Sentry for Homey",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "node build.mjs",
    "lint": "eslint",
    "fix": "eslint --fix",
    "jsdoc": "jsdoc --configure ./jsdoc.json",
    "jsdoc:serve": "serve ./jsdoc",
    "jsdoc:clean": "rm -rf ./jsdoc",
    "jsdoc:watch": "watch \"npm run jsdoc:clean && npm run jsdoc\" lib",
    "prettier": "prettier . --write",
    "prettier-check": "prettier . --check",
    "typings:generate": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drenso/homey-log.git"
  },
  "author": "Athom B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Drenso/homey-log/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/Drenso/homey-log#readme",
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@sentry/node-core": "10.39.0",
    "@types/homey": "npm:homey-apps-sdk-v3-types@0.3.12",
    "@types/node": "24.10.13",
    "esbuild": "0.27.3",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.3.0",
    "homey": "3.12.2",
    "jsdoc": "4.0.5",
    "jsdoc-ts-utils": "6.0.0",
    "npm-watch": "0.13.0",
    "prettier": "3.8.1",
    "serve": "14.2.5",
    "typescript": "5.9.3",
    "watch": "1.0.2",
    "ws": "8.19.0"
  },
  "watch": {
    "jsdoc": {
      "patterns": [
        "lib",
        "README.md"
      ],
      "extensions": [
        "js"
      ]
    }
  }
}
